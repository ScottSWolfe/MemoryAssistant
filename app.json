{
  "expo": {
    "name": "Memory Assistant",
    "description": "A very interesting project.",
    "slug": "MemoryAssistant",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.cs4261.memoryassistant",
      "supportsTablet": true
    },
    "android": {
      "package": "com.cs4261.memoryassistant"
    }
  }
}
